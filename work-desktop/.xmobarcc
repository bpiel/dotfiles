Config { font = "xft:dejavu sans mono:size=10:antialias=true"
, bgColor = "black"
, fgColor = "grey"
, position = Top
, lowerOnStart = False
, commands = [ Run Weather "KPHL" ["-t","<tempF>F <skyCondition>","-L","40","-H","75","--normal","green","--high","red","--low","lightblue"] 36000
             , Run Network "eth0" ["-L","0","-H","32","--normal","green","--high","red"] 10
             , Run Network "eth1" ["-L","0","-H","32","--normal","green","--high","red"] 10
             , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
             , Run Memory ["-t","Mem: <usedratio>%"] 10
             , Run Swap [ ] 10
             , Run Date "%a %b %_d %Y %I:%M:%S" "date" 10
             , Run StdinReader
	     , Run Com "python" ["/home/bill/bin/get-weather.py"] "weather1" 12000
             ]
, sepChar = "%"
, alignSep = "}{"
, template = "%StdinReader% }{ %weather1%  %KPHL% | <fc=#ee9a00>%date%</fc>"
}
